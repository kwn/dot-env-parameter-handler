{
    "name": "kwn/dot-env-parameter-handler-test",
    "description": "Composer.json for testing purposes",
    "license": "MIT",
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "DotEnvParameterHandler\\": "../../src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "DotEnvParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "DotEnvParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "dot-env-parameter-handler": {
            "source": ".env1.dist",
            "target": ".env",
            "strategy": "input"
        }
    }
}
