{
    "name": "kwn/dotenv-parameter-handler",
    "description": "Composer script building .env file from .env.dist",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Karol Wnuk",
            "email": "k.wnuk@ascetic.pl"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/dotenv": "^3.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.21",
        "mockery/mockery": "^0.9.9",
        "squizlabs/php_codesniffer": "^3.0.2",
        "codeclimate/php-test-reporter": "^0.4.4",
        "composer/composer": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "DotenvParameterHandler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotenvParameterHandler\\": [
                "tests/unit",
                "tests/functional"
            ]
        }
    }
}
